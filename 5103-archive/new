<script>
const leaveSiteBtn = document.getElementById("leave-site");
const warningPopup = document.getElementById("warning-popup");
const closeWarningBtn = document.getElementById("close-warning");
const stayOnSiteBtn = document.getElementById("stay-on-site");
const proceedAnywayBtn = document.getElementById("proceed-anyway");

leaveSiteBtn.addEventListener("click", (event) => {
  event.preventDefault();
  warningPopup.style.display = "block";
  document.body.style.overflow = "hidden";
});

closeWarningBtn.addEventListener("click", () => {
  warningPopup.style.display = "none";
  document.body.style.overflow = "auto";
});

stayOnSiteBtn.addEventListener("click", () => {
  warningPopup.style.display = "none";
  document.body.style.overflow = "auto";
});

proceedAnywayBtn.addEventListener("click", () => {
  window.location = leaveSiteBtn.href;
});

document.addEventListener("keydown", (event) => {
  if (event.key === "Escape") {
    warningPopup.style.display = "none";
    document.body.style.overflow = "auto";
  }
});
</script>
