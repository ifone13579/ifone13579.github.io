    <script>
     const MINE = "ðŸ’£";
const FLAG = "ðŸš©";

class Cell {
  constructor(row, col, isMine) {
    this.row = row;
    this.col = col;
    this.isMine = isMine;
    this.isRevealed = false;
    this.isFlagged = false;
    this.surroundingMines = 0;
  }
}

class Board {
  constructor(rows, cols, mines) {
    this.rows = rows;
    this.cols = cols;
    this.mines = mines;
    this.cells = [];
    this.generateBoard();
  }

  generateBoard() {
    // Generate cells and place mines
    for (let row = 0; row < this.rows; row++) {
      this.cells[row] = [];
      for (let col = 0; col < this.cols; col++) {
        this.cells[row][col] = new Cell(row, col, false);
      }
    }
    let placedMines = 0;
    while (placedMines < this.mines) {
      let row = Math.floor(Math.random() * this.rows);
      let col = Math.floor(Math.random() * this.cols);
      if (!this.cells[row][col].isMine) {
        this.cells[row][col].isMine = true;
        placedMines++;
      }
    }

    // Count surrounding mines for each cell
    for (let row = 0; row < this.rows; row++) {
      for (let col = 0; col < this.cols; col++) {
        let cell = this.cells[row][col];
        if (!cell.isMine) {
          let mines = 0;
          for (let i = -1; i <= 1; i++) {
            for (let j = -1; j <= 1; j++) {
              if (
                row + i >= 0 &&
                row + i < this.rows &&
                col + j >= 0 &&
                col + j < this.cols
              ) {
                let surroundingCell = this.cells[row + i][col + j];
                if (surroundingCell.isMine) {
                  mines++;
                }
              }
            }
          }
          cell.surroundingMines = mines;
        }
      }
    }
  }

  reveal(row, col) {
    let cell = this.cells[row][col];
    if (!cell.isRevealed && !cell.isFlagged) {
      cell.isRevealed = true;
      if (cell.isMine) {
        alert("Game Over!");
      } else if (cell.surroundingMines === 0) {
        for (let i = -1; i <= 1; i++) {
          for (let j = -1; j <= 1; j++) {
            if (
              row + i >= 0 &&
              row + i < this.rows &&
              col + j >= 0 &&
              col + j < this.cols
            ) {
              this.reveal

      function startMinesweeper() {
  // Initialize game board
  let board = createBoard();

  // Start game logic
  playGame(board);
}

// Start game when page is loaded
window.onload = startMinesweeper;

      }
      startMinesweeper();
    </script>
